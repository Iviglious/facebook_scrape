                              "type": "string"
                            }
                          }
                        }
                      }
                    }
                  },
                  "required": [
                    "media"
                  ]
                }
              }
            }
          }
        },
        "required": [
          "id",
          "object_id",
          "created_time",
          "updated_time",
          "story",
          "from",
          "attachments"
        ]
      }
    },
    "paging": {
      "type": "object",
      "properties": {
        "cursors": {
          "type": "object",
          "properties": {
            "before": {
              "type": "string"
